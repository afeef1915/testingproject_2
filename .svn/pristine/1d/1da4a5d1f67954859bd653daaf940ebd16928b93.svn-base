.navbar-mtl {
    background: #6C49A7;
    box-shadow: 0 0 0 0 rgba(0, 0, 0, 0.09), 0 0 3px 0 rgba(0, 0, 0, 0.12)#383F46;
}

.top-nav > li > a {
    color: #FFFFFF;
    background: #6C49A7;
}

#quick_search_input {
    color: #FFFFFF;
}

.top-nav > li > a:hover, 
.top-nav > li > a:focus, 
.top-nav > .open > a, 
.top-nav > .open > a:hover, 
.top-nav > .open > a:focus {
    color: #FFFFFF;
    background: #63439A;
}

.top_search_box .btn,
.top_search_box .form-control {
    background-color: transparent;
}
.top_search_box .btn,
.top_search_box input {
    border: none;
    box-shadow: none;
}

.top_search_box > .input-group {
    border-bottom: 1px solid #B6A4D3;
}

.top_search_box .fa,
.top_search_box .caret {
    color: #B6A4D3;
}

.sidebar-nav {
    border-bottom: 1px solid #585D64;
    background-image: -webkit-linear-gradient(left, color-stop(#2E343D 5px), color-stop(#2E343D 5px));
    background-image: linear-gradient(to right, #2E343D 5px, #2E343D 5px);
}

.sidebar-nav ul ul a:hover, 
.sidebar-nav ul ul a:focus {
    background-color: #585D64;
    background-image: -webkit-linear-gradient(left, color-stop(#393F47 5px), color-stop(#393F47 5px));
    background-image: linear-gradient(to right, #393F47 5px, #393F47 5px);
}

.sidebar ul li a.active {
    color: #EFF0F1;
    background-color: #393F47;
    background-image: -webkit-linear-gradient(left, color-stop(#EFF0F1 5px), color-stop(#393F47 5px));
    background-image: linear-gradient(to right, #EFF0F1 5px, #393F47 5px);
}

.breadcrumb {
    background: #EFF0F1;
    color: #ADADAD;
}

.breadcrumb > .active {
    color: #ADADAD;
}


@media(max-width:767px) {
    .top-nav, .top-nav > li > a {
        background-image: -webkit-linear-gradient(left, color-stop(#2E343D 5px), color-stop(#2E343D 5px));
        background-image: linear-gradient(to right, #2E343D 5px, #2E343D 5px);
    }

    .top_nav.navbar-mtl .navbar-toggle .icon-bar {
        color: #FFFFFF;
        background-color: #FFFFFF;
    }
    .top_nav.navbar-mtl .navbar-toggle:hover {
        background-color: #63439A;
    }

    .top_nav.navbar-mtl .navbar-toggle:focus span {
        background: #FFFFFF;
    }

    .top_search_box .fa, .top_search_box .caret {
        color: #666666;
    }

    .top_search_box > .input-group {
        border-bottom: transparent;
    }    

    .top_search_box .btn:active, 
    .top_search_box input:active,
    .top_search_box .btn:focus, 
    .top_search_box input:focus {
        background-color: #ffffff !important;
    }

    .top_search_box .btn:active .top_search_box .fa, 
    .top_search_box input:active .top_search_box .fa,
    .top_search_box .btn:focus .top_search_box .fa, 
    .top_search_box input:focus .top_search_box .fa {
        color: red !important;
    }
}
