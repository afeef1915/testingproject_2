                var app_theme_color_1 = '{{ app_theme_color1 }}';
                var app_theme_color_2 = '{{ app_theme_color2 }}';
                var UserTheme = '{{ user_settings is defined ? user_settings.theme | eserv_get_user_theme('main') : 'mtl' }}';
                var UserColorTheme = '{{ user_settings is defined ? user_settings.theme | eserv_get_user_theme('color') : 'DEFAULT' }}';
                var UserScreenWidth = '{{ UserScreenWidth is defined ? UserScreenWidth : 'normal' }}';
                var ClientDefaultTheme = '{{ app_client_short_name | trans }} Default';
                var appAlertTimeOut = 20000;
                var theme_folder = '{{ asset ('common/assets/css/themes/'~ app_theme ~'/') }}';
                var client_theme_folder = '{{ asset ('clients/'~ app_client ~'/themes/'~ app_theme ~'/') }}';
                var PreloaderImagePath = '{{ asset ('clients/'~ app_client ~'/themes/'~ app_theme ~'/images/main/processing_data.gif') }}?v={{ app_version }}_{{ NOW }}';
                var QtipPreloaderImage = '{{ asset ('common/assets/images/icons/qtip_preloader.gif') }}';
            {% else %}
                var isAuthenticated = false;
            {% endif %}
        </script>
        <script src="{{ asset ('common/assets/libs/merlin/jquery/jquery.min.js') }}?v={{ app_version }}"></script>
        <script src="{{ asset ('common/assets/libs/merlin/jquery-ui/js/jquery-ui.min.js') }}?v={{ app_version }}"></script>
        <script src="{{ asset ('common/assets/libs/merlin/jquery-cookie/jquery.cookie.js') }}?v={{ app_version }}_{{ NOW }}"></script>
        <script src="{{ asset ('common/assets/libs/merlin/bootstrap/js/bootstrap.min.js') }}?v={{ app_version }}_{{ NOW }}"></script>
        <script src="{{ asset ('common/assets/libs/merlin/bootstrap-switch/js/bootstrap-switch.min.js') }}?v={{ app_version }}_{{ NOW }}"></script>
        <script src="{{ asset ('common/assets/libs/merlin/jquery-form/jquery.form.js') }}?v={{ app_version }}_{{ NOW }}"></script>
        <script src="{{ asset ('common/assets/libs/merlin/jquery-qtip/js/jquery.qtip.min.js') }}"></script>
        <script src="{{ asset ('common/assets/libs/merlin/angular/angular.min.js') }}?v={{ app_version }}_{{ NOW }}"></script>
        <script src="{{ asset ('common/assets/libs/merlin/angular/angular-sanitize.min.js') }}?v={{ app_version }}_{{ NOW }}"></script>
        <script src="{{ asset ('common/assets/libs/merlin/angular-ui-router/release/angular-ui-router.min.js') }}?v={{ app_version }}_{{ NOW }}"></script>
        <script src="{{ asset ('common/assets/libs/merlin/angular-extra-modules/ui-bootstrap-tpls.min.js') }}?v={{ app_version }}_{{ NOW }}"></script>
        <script src="{{ asset ('common/assets/libs/merlin/angular-extra-modules/angular-breadcrumb.min.js') }}?v={{ app_version }}_{{ NOW }}"></script>
        <script src="{{ asset ('common/assets/libs/merlin/angular-extra-modules/dialogs.min.js') }}?v={{ app_version }}_{{ NOW }}"></script>
        <script src="{{ asset ('common/assets/libs/merlin/select2/dist/js/select2.min.js') }}?v={{ app_version }}_{{ NOW }}"></script>
        <script src="{{ asset ('common/assets/libs/merlin/jquery-metisMenu/js/metisMenu.min.js') }}?v={{ app_version }}_{{ NOW }}"></script>
        <script src="{{ asset ('common/assets/libs/merlin/jquery-file-upload/js/file_upload_all.js') }}?v={{ app_version }}_{{ NOW }}"></script>

        {#--start of graphs js #}
        {#
        Samir 12.Feb.2017: Removed invalid if condition below
        #}
        {#% if app.request.uri~'#/dashboard/data'  %#}
        <script src="{{ asset ('common/assets/libs/raphael/raphael.min.js') }}"></script>
        <script src="{{ asset ('common/assets/libs/morris/morris.min.js') }}"></script>
        {# <script src="{{ asset ('common/assets/libs/morris/morris.unmin.js') }}"></script>#}
        {#%endif%#}
        {#--end of graphs js #}

        {% javascripts '@data_tables_js' %}
        <script type="text/javascript" src="{{ asset_url }}?v={{ app_version }}_{{ NOW }}"></script>
        {% endjavascripts %}
        <script src="{{ asset ('common/assets/libs/merlin/merlin/js/eserv_functions.js') }}?v={{ app_version }}_{{ NOW }}"></script>
        <script src="{{ asset ('common/assets/js/app.js') }}?v={{ app_version }}_{{ NOW }}"></script>
        <script src="{{ asset ('common/assets/js/extras.js') }}?v={{ app_version }}_{{ NOW }}"></script>
    </body>
</html>
