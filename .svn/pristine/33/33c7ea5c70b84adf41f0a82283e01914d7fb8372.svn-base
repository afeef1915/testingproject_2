#page-wrapper {
    background: rgba(0, 0, 0, 0) linear-gradient(to right, rgb(34, 41, 46) 0%, rgb(34, 41, 46) 229px, #f6f6f6 229px) repeat scroll 0 0;
}

.sidebar {
    background: transparent;
}
.navbar-mtl {
    background: rgb(255, 255, 255);
    box-shadow: 0 0 0 0 rgba(0, 0, 0, 0.09), 0 0 3px 0 rgba(0, 0, 0, 0.09);
}

.nano .nano-pane {
    background: rgba(255, 255, 255, 0.4) none repeat scroll 0 0;
}
.nano > .nano-pane > .nano-slider {
    background: rgba(0, 0, 0, 0.4) none repeat scroll 0 0;
}

.top-nav > li > a {
    color: rgb(106, 106, 106);
    background: rgb(255, 255, 255);
}

#quick_search_input {
    color: rgb(106, 106, 106);
}

.top-nav > li > a:hover, 
.top-nav > li > a:focus, 
.top-nav > .open > a, 
.top-nav > .open > a:hover, 
.top-nav > .open > a:focus {
    color: rgb(106, 106, 106);
    background: rgb(245, 246, 247);
}

.top_search_box .btn,
.top_search_box .form-control {
    background-color: transparent;
}
.top_search_box .btn,
.top_search_box input {
    border: none;
    box-shadow: none;
}

.top_search_box > .input-group {
    border-bottom: 1px solid rgb(166, 166, 166);
}

.top_search_box .fa,
.top_search_box .caret {
    color: rgb(166, 166, 166);
}

.top_search_box .btn:active, 
.top_search_box input:active,
.top_search_box .btn:focus, 
.top_search_box input:focus {
    background-color: rgb(255, 255, 255) !important;
}

.form-control::-moz-placeholder {
    color: rgb(166, 166, 166);
    opacity: 0.65;
    filter: 65%;
}

.sidebar-nav {
    border-bottom: none;
    background-image: -webkit-linear-gradient(left, color-stop(rgb(34, 41, 46) 5px), color-stop(rgb(34, 41, 46) 5px));
    background-image: linear-gradient(to right, rgb(34, 41, 46) 5px, rgb(34, 41, 46) 5px);
    background-repeat: repeat-x;
    box-shadow: 2px 0 37px 1px rgba(0, 0, 0, 0.12);
}

.sidebar-nav ul a {
    color: rgb(190, 190, 190);
    border-top: 1px solid rgba(0, 0, 0, 0.3); 
    box-shadow: 0px 1px 0px rgba(255, 255, 255, 0.05) inset;
    text-shadow: none;
}
.sidebar-nav ul a:hover, .sidebar-nav ul a:focus, .sidebar-nav ul a:active {
    color: rgb(255, 255, 255);
    background-color: rgb(45, 51, 57);
    background-image: -webkit-linear-gradient(left, color-stop(rgb(240, 133, 67) 5px), color-stop(rgb(34, 41, 46) 5px));
    background-image: linear-gradient(to right, rgb(240, 133, 67) 5px, rgb(34, 41, 46) 5px);
}
.sidebar-nav ul ul a {
    color: rgb(190, 190, 190);
    background-color: rgb(45, 51, 57);
}
.sidebar-nav ul ul a:hover, .sidebar-nav ul ul a:focus, .sidebar-nav ul ul a:active {
    background-color: rgb(45, 51, 57);
    background-image: -webkit-linear-gradient(left, color-stop(rgb(240, 133, 67) 5px), color-stop(rgb(34, 41, 46) 5px));
    background-image: linear-gradient(to right, rgb(240, 133, 67) 5px, rgb(34, 41, 46) 5px);
}

.sidebar ul li a.active {
    color: rgb(255, 255, 255);
    background-color: rgb(45, 51, 57);
    background-image: -webkit-linear-gradient(left, color-stop(rgb(240, 133, 67) 5px), color-stop(rgb(34, 41, 46) 5px));
    background-image: linear-gradient(to right, rgb(240, 133, 67) 5px, rgb(34, 41, 46) 5px);
}

.main_menu_navigation_label {
    background: rgb(34, 41, 46);
    color: rgb(102, 102, 102);
}

.breadcrumb {
    background: rgb(239, 240, 241);
    color: rgb(173, 173, 173);
}

.breadcrumb > .active {
    color: rgb(173, 173, 173);
}

.dashboard_stats_squares .panel {
    box-shadow: 0 1px 1px rgba(0, 9, 255, 0.09);
}

.dashboard_stats_squares .panel.bg-success .panel-body {
    background: rgb(71, 217, 144);
}

.dashboard_stats_squares .panel.bg-success .panel-body .dashboard-stat-content,
.dashboard_stats_squares .panel.bg-success .panel-body .dashboard-stat-content .stats_numbers {
    color: rgb(255, 255, 255);
}
.dashboard_stats_squares .panel.bg-success .dashboard-icon {
    color: rgba(255, 255, 255, 0.5);
    background: rgb(64, 196, 130);
}

.dashboard_stats_squares .panel.bg-warning .panel-body {
    background: rgb(252, 208, 76);
}

.dashboard_stats_squares .panel.bg-warning .panel-body .dashboard-stat-content,
.dashboard_stats_squares .panel.bg-warning .panel-body .dashboard-stat-content .stats_numbers {
    color: rgb(255, 255, 255);
}
.dashboard_stats_squares .panel.bg-warning .dashboard-icon {
    color: rgba(255, 255, 255, 0.5);
    background: rgb(230, 190, 69);
}

.dashboard_stats_squares .panel.bg-info .panel-body {
    background: rgb(73, 133, 224);
}

.dashboard_stats_squares .panel.bg-info .panel-body .dashboard-stat-content,
.dashboard_stats_squares .panel.bg-info .panel-body .dashboard-stat-content .stats_numbers {
    color: rgb(255, 255, 255);
}
.dashboard_stats_squares .panel.bg-info .dashboard-icon {
    color: rgba(255, 255, 255, 0.5);
    background: rgb(60, 109, 184);
}

.dashboard_stats_squares .panel.bg-purple .panel-body {
    background: rgb(127, 71, 217);
}

.dashboard_stats_squares .panel.bg-purple .panel-body .dashboard-stat-content,
.dashboard_stats_squares .panel.bg-purple .panel-body .dashboard-stat-content .stats_numbers {
    color: rgb(255, 255, 255);
}
.dashboard_stats_squares .panel.bg-purple .dashboard-icon {
    color: rgba(255, 255, 255, 0.5);
    background: rgb(115, 64, 197);
}

.portlet {
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.3);
}

.portlet-header {
    color: rgb(245, 245, 245);
    border-color: rgb(0, 0, 0);
    background: rgb(70, 77, 80);
    background: -moz-linear-gradient(top,  rgb(70, 77, 80) 0%, rgb(49, 57, 60) 100%);
    background: -webkit-linear-gradient(top,  rgb(70, 77, 80) 0%,rgb(49, 57, 60) 100%);
    background: linear-gradient(to bottom,  rgb(70, 77, 80) 0%,rgb(49, 57, 60) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='rgb(70, 77, 80)', endColorstr='rgb(49, 57, 60)',GradientType=0 );
}

.portlet-header .btn-group a span {
    color: rgb(245, 245, 245);
}

.portlet .portlet-header .header_icon {
    color: rgb(245, 245, 245);
}

.portlet-content {
    color: rgb(93, 93, 93);
    background: rgb(255, 255, 255);
}

.nav-tabs > li.active > a, .nav-tabs > li.active > a:focus, .nav-tabs > li.active > a:hover {
    color: rgb(93, 93, 93);
    background: rgb(255, 255, 255);
    border-top-color: rgb(240, 133, 67);
}

.modal-header {
    color: rgb(245, 245, 245);
    border-color: rgb(211, 101, 30);
    background: rgb(239, 114, 34);
    background: -moz-linear-gradient(top,  rgb(239, 114, 34) 0%, rgb(211, 101, 30) 100%);
    background: -webkit-linear-gradient(top,  rgb(239, 114, 34) 0%,rgb(211, 101, 30) 100%);
    background: linear-gradient(to bottom,  rgb(239, 114, 34) 0%,rgb(211, 101, 30) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='rgb(239, 114, 34)', endColorstr='rgb(211, 101, 30)',GradientType=0 );
}

@media(max-width:767px) {
    #page-wrapper {
        background: #f6f6f6;
    }
    .top-nav, .top-nav > li > a {
        color: rgb(255, 255, 255);
        background-image: -webkit-linear-gradient(left, color-stop(rgb(34, 41, 46) 5px), color-stop(rgb(34, 41, 46) 5px));
        background-image: linear-gradient(to right, rgb(34, 41, 46) 5px, rgb(34, 41, 46) 5px);
    }

    .top_nav.navbar-mtl .navbar-toggle .icon-bar {
        color: rgb(106, 106, 106);
        background: rgb(106, 106, 106) !important;
    }
    .top_nav.navbar-mtl .navbar-toggle:hover {
        background: rgb(245, 246, 247);
    }

    .top_nav.navbar-mtl .navbar-toggle:focus span {
        background: rgb(106, 106, 106);
    }

    .quick_search_box .controls {
        background: rgb(239, 240, 241);
    } 

    .top_search_box .fa, .top_search_box .caret {
        color: #666666;
    }

    .top_search_box > .input-group {
        border-bottom: transparent;
    }    

    .top_search_box .btn:active, 
    .top_search_box input:active,
    .top_search_box .btn:focus, 
    .top_search_box input:focus {
        background-color: rgb(239, 240, 241) !important;
    }
}
@media (min-width: 768px) and (max-width: 992px) {
    #page-wrapper {
        background: rgba(0, 0, 0, 0) linear-gradient(to right, rgb(34, 41, 46) 0%, rgb(34, 41, 46) 65px, #f6f6f6 65px) repeat scroll 0 0;
    }

    /*
.sidebar-nav ul a:hover, .sidebar-nav ul a:focus, .sidebar-nav ul a:active {
    color: #fff;
    background-color: #2D3339;
    background-image: -webkit-linear-gradient(left, color-stop(rgb(0, 85, 64) 5px), color-stop(rgb(249, 249, 249) 5px));
    background-image: linear-gradient(to right, rgb(0, 85, 64) 5px, rgb(249, 249, 249) 5px);
}
    */

    .sidebar-nav ul a:hover, .sidebar-nav ul a:focus, .sidebar-nav ul a:active {
        color: rgb(255, 255, 255);
        background-color: rgb(45, 51, 57);
        background-image: -webkit-linear-gradient(left, color-stop(rgba(0, 0, 0, 0.3) 5px), color-stop(rgb(45, 51, 57) 5px));
        background-image: linear-gradient(to right, rgba(0, 0, 0, 0.3) 5px, rgb(45, 51, 57) 5px);
        border-color: rgba(0, 0, 0, 0.3);
    }

    #app_main_menu li:hover > ul, 
    #app_main_menu li:hover > ul.collapse {
        background: #2D3339;
    }
}


