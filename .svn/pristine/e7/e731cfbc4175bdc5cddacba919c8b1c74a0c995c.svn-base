/*--------------- Begin Tables --------------- */
/* Table: border between rows */
.table > thead > tr > th, 
.table > tbody > tr > th, 
.table > tfoot > tr > th, 
.table > thead > tr > td, 
.table > tbody > tr > td, 
.table > tfoot > tr > td {
    border-color: #eee;
}
table.table thead .sorting,
table.table thead .sorting_asc,
table.table thead .sorting_desc,
table.table thead .sorting_asc_disabled,
table.table thead .sorting_desc_disabled {
    background-repeat: no-repeat !important;
    background-position: center right !important;
    background-color: #fff !important; /* Header background color */
    border-color: transparent; /* Header top, right and left border colors */
    color: #C59600; /* Header text color */
    border-bottom: 1px solid #ccc !important; /* Header bottom line (border) :-) */
}

.table > caption + thead > tr:first-child > th, 
.table > colgroup + thead > tr:first-child > th, 
.table > thead:first-child > tr:first-child > th, 
.table > caption + thead > tr:first-child > td, 
.table > colgroup + thead > tr:first-child > td, 
.table > thead:first-child > tr:first-child > td {
    color: #C59600;
    background-color: #fff;
    border-color: transparent;
    border-bottom-color: #ccc;
}

table.table thead .sorting_asc,
table.table thead .sorting_desc {
    background-color: #fff !important;
    border-color: transparent;
    color: #C59600;
    border-bottom: 3px solid #C59600 !important;
}

table.table thead .sorting { background-image: url('images/gold/datatables/sort_both.png') !important; }
table.table thead .sorting_asc { background-image: url('images/gold/datatables/sort_asc.png') !important; }
table.table thead .sorting_desc { background-image: url('images/gold/datatables/sort_desc.png') !important; }

table.table thead .sorting_asc_disabled { background-image: url('images/gold/datatables/sort_asc_disabled.png') !important; }
table.table thead .sorting_desc_disabled { background-image: url('images/gold/datatables/sort_desc_disabled.png') !important; }

.pagination > .active > a, .pagination > .active > span, .pagination > .active > a:hover, .pagination > .active > span:hover, .pagination > .active > a:focus, .pagination > .active > span:focus {
    transition: all 0.4s ease 0s !important;
    background-color: #C59600 !important;
    border-color: #C59600 !important;
    color: #282720 !important;
}
/*--------------- End Tables --------------- */

/*--------------- Begin Portlets --------------- */
.portlet-header {
