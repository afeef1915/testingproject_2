body {
    background-color: #222;
    background: #ccc;
    /*    background-image: url('../images/bgs/body_bg.png');*/
}

#page-wrapper {
    box-shadow: 0 0 5px rgba(0,0,0,.15);
}

/* Top Navigation */
.top-nav>li>a {
    color: #999;
}

.top-nav>li>a:hover,
.top-nav>li>a:focus,
.top-nav>.open>a,
.top-nav>.open>a:hover,
.top-nav>.open>a:focus {
    color: #fff;
    background-color: #000;
}

.top-nav>.open>.dropdown-menu {
    border: 1px solid rgba(0,0,0,.15);
    background-color: #fff;
    -webkit-box-shadow: 0 6px 12px rgba(0,0,0,.175);
    box-shadow: 0 6px 12px rgba(0,0,0,.175);
}

.brand_logo.main {
    background-image: url('../images/logos/client_logo.png');
}

.brand_logo.signin {
    background-image: url('../images/logos/login_screen_logo.png');
}

li.message-preview {
    border-bottom: 1px solid rgba(0,0,0,.15);
}

/* Side Navigation */
.sidebar-nav {
    border-bottom: 1px solid rgba(0, 0, 0, 0.3);
    background-image: -webkit-linear-gradient(left, color-stop(#215B9B 5px), color-stop(#222222 5px));
    background-image: linear-gradient(to right, #215B9B 5px, #222222 5px);
}

.sidebar-nav ul ul a:hover, .sidebar-nav ul ul a:focus, .sidebar-nav ul ul a:active {
    background-color: rgba(255, 255, 255, 0.2);
    background-image: -webkit-linear-gradient(left, color-stop(#EF762E 5px), color-stop(#222222 5px));
    background-image: linear-gradient(to right, #EF762E 5px, #222222 5px);
}

.sidebar ul li a.active {
    color: #fff;
    background-color: rgba(255, 255, 255, 0.2);
    background-image: -webkit-linear-gradient(left, color-stop(#EF762E 5px), color-stop(#111111 5px));
    background-image: linear-gradient(to right, #EF762E 5px, #111111 5px);
}

