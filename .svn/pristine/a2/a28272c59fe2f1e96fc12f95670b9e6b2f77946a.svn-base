img.login_screen_pattern {
    /* Set rules to fill background */
    min-height: 100%;
    min-width: 1024px;

    /* Set up proportionate scaling */
    width: 100%;
    height: auto;

    /* Set up positioning */
    position: fixed;
    top: 0;
    left: 0;

    z-index: -1;
}

@media screen and (max-width: 1024px){
    img.login_screen_pattern {
        left: 50%;
        margin-left: -512px; }
}

