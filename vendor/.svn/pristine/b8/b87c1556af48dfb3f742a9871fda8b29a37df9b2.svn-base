.dt-button-background{
    background: #000;
    position:fixed;
    width:100%;
    height:100%;
    left:0%;
    top:0%;
    z-index: 1029;
    opacity: .90;
    filter:Alpha(Opacity=90);
}

.dt-button-collection {
    max-width: 713px;
    max-height: 370px;
    overflow: auto;
    background-color: #fff;
    position: fixed;
    top: 250px;
    left: 0;
    right: 0;
    margin: auto;
    padding: 15px;
    z-index: 1030;
    border: 10px solid #ccc;
}

.dt-button-collection a.dt-button,
.dt-button-collection button.dt-button {
    -moz-user-select: none;
    border: 1px solid #fff;
    cursor: pointer;
    display: inline-block;
    font-size: 14px;
    font-weight: 400;
    line-height: 1.42857;
    margin-bottom: 0;
    padding: 6px 12px;
    text-align: left;
    vertical-align: middle;
    white-space: nowrap;
    background-color: #eee;
    color: #333;
    min-width: 33.33333%;
    text-decoration: none;
    cursor: pointer;
}

@media (max-width: 767px) {
    .dt-button-collection a.dt-button, .dt-button-collection button.dt-button {
        min-width: 100%;
    }
}

.dt-button-collection a.dt-button:hover,
.dt-button-collection button.dt-button:hover {
    background-color: #fff;
    border-color: #ccc;
}

.dt-button-collection a.dt-button span:before,
.dt-button-collection button.dt-button span:before {
    color: #ccc;
    font-family: 'FontAwesome';
    content: "\f14a       ";
}

.dt-button-collection a.dt-button.active span:before,
.dt-button-collection button.dt-button.active span:before {
    color: green;
    font-family: 'FontAwesome';
    content: "\f14a       ";
}

.dt-buttons a, .dt-buttons button {
    padding: 4px 12px;
}

.dt-button-collection .dt-buttons-header {
    margin: -15px -15px 10px;
    padding: 10px 15px 5px;
    background: #eee;
    border: none;
    border-bottom: 1px solid #ccc;
}

div.dataTables_length label {
    float: left;
    text-align: left;
}

.inner_datatable label{
    width: auto !important;
}

.inner_datatable .table > caption + thead > tr:first-child > th, .inner_datatable .table > colgroup + thead > tr:first-child > th, .inner_datatable .table > thead:first-child > tr:first-child > th, .inner_datatable .table > caption + thead > tr:first-child > td, .inner_datatable .table > colgroup + thead > tr:first-child > td, .inner_datatable .table > thead:first-child > tr:first-child > td {
    background: #ebcccc !important;
    color: #333 !important;
}
div.dataTables_length select {
    width: 75px;
}

div.dataTables_filter label {
    float: right;
}

div.dataTables_info {
    padding-top: 26px;
}

div.dataTables_paginate {
    float: right;
    margin: 0;
}

table.table {
