## Alternative Language
eserv_main_administration_alt_lang_selected:
    path:     admin/alt_lang/select_lang/{alt_lang_id}
    defaults: { _controller: ESERVMAINAdministrationBundle:AltLang:altLangList, alt_lang_id: null }
    
eserv_main_administration_alt_lang_select:
    path:     admin/alt_lang/select
    defaults: { _controller: ESERVMAINAdministrationBundle:AltLang:selectLanguage }
    
