    path:     admin/alt_lang/ajaxList/{alt_lang_id}
    defaults: { _controller: ESERVMAINAdministrationBundle:AltLang:data, alt_lang_id: null }    
    
eserv_main_administration_alt_lang_new:
    path:     admin/alt_lang/add
    defaults: { _controller: ESERVMAINAdministrationBundle:AltLang:newAltLang }
    
eserv_main_administration_alt_lang_new_with_lang:
    path:     admin/alt_lang/{alt_lang_id}/add
    defaults: { _controller: ESERVMAINAdministrationBundle:AltLang:newAltLang, alt_lang_id: null }

eserv_main_administration_alt_lang_create:
    path:     admin/alt_lang/create/{alt_lang_id}
    defaults: { _controller: ESERVMAINAdministrationBundle:AltLang:createAltLang, alt_lang_id: null }

