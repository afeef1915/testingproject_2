{% include 'ESERVMAINHelpersBundle:Form:required_msg.html.twig' with {'viewMode': 'view'} %}
<div id="eserv_AddContactStatus_output"></div>

{{form_start(form, { 'attr' : { 'id': 'AddContactStatus' } })}}

<div class="eserv_container_form_errors"></div>
<fieldset>
    <div class="col-lg-12">
        {{ form_eservSelectRow (form.contactType) }}
        {{ form_eservSelectRow (form.statusType) }}
        {{ form_eservRow (form.code) }}
        {{ form_eservRow (form.name) }}
        {{ form_eservSelectRow (form.mtlColour) }}
    </div>
</fieldset>

<fieldset>
    {% include 'ESERVMAINHelpersBundle:Form:form_general_btns.html.twig' with { 'form_save' : form.save, 'hide_edit_button' : true, 'hide_cancel' : true, 'cancel_link' : true,  'cancelHref' : '#/admin/contact-status', 'closeBtnId' : 'cancel_AddContactStatus_btn' } %}                                                                             
</fieldset>   

<div style="display: none">
    {{form_rest(form)}}
</div>

{{form_end(form)}}

<script type="text/javascript">
    $(document).ready(function () {
        prepareAppForms('{{ form.vars.name }}', false, false, false,
                false, function () {
                    $('#cancel_AddContactStatus_btn').click();
                });
    });
</script>