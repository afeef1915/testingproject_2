ESERV\MAIN\CommunicationsBundle\Entity\PdfMergingArchive:
    type: entity
    table: pdf_merging_archive
    id:
        id:
            type: integer
            generator: { strategy: AUTO }
    fields:
        size:
            type: integer
            length: 4            
        filename:
            type: string
        content:
            type: blob
        pdf_merging_lookup_archive_id:
            type: integer
            length: 4
        external_id:
            type: string            
        created_at:
            type: datetime
        updated_at:
            type: datetime
        created_by:
            type: bigint
        updated_by:
            type: bigint